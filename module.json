{
  "id": "pf2e-feat-filter",
  "title": "PF2e Feat Filter",
  "version": "#{VERSION}#",
  "compatibility": {
    "minimum": "13",
    "verified": "13.350"
  },
  "esmodules": [
    "module.js"
  ],
  "styles": [
    "styles/feat-filter.css"
  ],
  "authors": [
    {
      "name": "kromko",
      "flags": {}
    }
  ],
  "flags": {
    "canUpload": true,
    "pf2e-feat-filter": {
      "files": [
        "pf2e-playtest-data",
        "pf2e",
        "sf2e-anachronism",
        "starfinder-field-test-for-pf2e"
      ]
    }
  },
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json",
      "flags": {}
    },
    {
      "lang": "pl",
      "name": "Polski",
      "path": "lang/pl.json",
      "flags": {}
    }
  ],
  "relationships": {
    "systems": [
      {
        "id": "pf2e",
        "type": "system",
        "manifest": "https://raw.githubusercontent.com/foundryvtt/pf2e/master/system.json",
        "compatibility": {
          "minimum": "7.7",
          "verified": "7.7.1"
        }
      }
    ],
    "requires": [
      {
        "id": "lib-wrapper",
        "manifest": "https://github.com/ruipin/fvtt-lib-wrapper/releases/latest/download/module.json",
        "type": "module",
        "compatibility": {}
      }
    ]
  },
  "description": "<p>Small module to hide feats incompatible with a selected character.</p>",
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "changelog": "https://github.com/Cuingamehtar/fvtt-pf2e-feat-filter/blob/main/CHANGELOG.md"
}